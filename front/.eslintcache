storicos/:nif\" render={(props) => <DetalhesHistorico {...props} data=\"\"/>}/>\n            <Route path=\"/funcionarios-cadastrados\" component={Funcionarios}/>\n            <Route path=\"/cadastro\" component={Cadastro} />\n            <Route path=\"/perfil-adm\" component={PerfilAdm} />\n            <Route path=\"/historico-adm\" component={HistoricoAdm} />\n        </Switch>\n    </BrowserRouter>\n)\n\nexport default Routes;","/home/deny/Desktop/Projetos/Reppografia_SENAI/front/src/pages/Perfil_Adm/index.jsx",["131","132","133","134","135","136","137"],"/home/deny/Desktop/Projetos/Reppografia_SENAI/front/src/pages/Formulario/styles.js",[],"/home/deny/Desktop/Projetos/Reppografia_SENAI/front/src/pages/Mesa_Grafico/styles.js",[],"/home/deny/Desktop/Projetos/Reppografia_SENAI/front/src/pages/Cadastro/styles.js",[],"/home/deny/Desktop/Projetos/Reppografia_SENAI/front/src/components/Tabela_Registros/styles.js",[],"/home/deny/Desktop/Projetos/Reppografia_SENAI/front/src/components/Header/index.jsx",[],"/home/deny/Desktop/Projetos/Reppografia_SENAI/front/src/components/InfoHistorico/index.jsx",[],"/home/deny/Desktop/Projetos/Reppografia_SENAI/front/src/components/Tabela_Registros/index.jsx",["138","139"],"/home/deny/Desktop/Projetos/Reppografia_SENAI/front/src/components/Header/styles.js",[],"/home/deny/Desktop/Projetos/Reppografia_SENAI/front/src/components/Graficos/styles.js",[],"/home/deny/Desktop/Projetos/Reppografia_SENAI/front/src/components/Tabela_De_Historico/styles.js",[],"/home/deny/Desktop/Projetos/Reppografia_SENAI/front/src/components/Button/styles.js",[],"/home/deny/Desktop/Projetos/Reppografia_SENAI/front/src/components/SaibaMais/styles.js",[],"/home/deny/Desktop/Projetos/Reppografia_SENAI/front/src/components/Tabela_De_Historico/index.jsx",["140","141","142"],"import React, { Component, useState } from 'react';\nimport ReactPaginate from 'react-paginate'\nimport SaibaMais from '../SaibaMais'\nimport { Container, Info, Tabela, Title, Pesquisa, Input } from './styles';\nimport axios from 'axios'\nimport Cookies from 'universal-cookie'\n\nconst cookies = new Cookies()\n\n\n\n\nexport class Tabela_De_Historico extends Component {\n\n  //Iniciando o componente com o construtor recebendo as props e iniciando os estados\n  constructor(props) {\n    super(props)\n\n    this.state = {\n      offset: 0,\n      tableData: [],\n      orgtableData: [],\n      perPage: 5,\n      currentPage: 0,\n      search: '',\n\n      User_info: {},\n      Requisicoes: {}\n\n    }\n    this.handlePageClick = this.handlePageClick.bind(this);\n\n  }\n\n\n\n  //funcao para fazer a ppaginacao funciona\n  handlePageClick = (e) => {\n    const selectedPage = e.selected;\n    const offset = selectedPage * this.state.perPage;\n\n    this.setState({\n      currentPage: selectedPage,\n      offset: offset\n    }, () => {\n      this.loadMoreData()\n    });\n\n  };\n\n  //Caregar mais pessoas quando mudas de paginacao\n  loadMoreData() {\n    const data = this.state.orgtableData;\n\n    const slice = data.slice(this.state.offset, this.state.offset + this.state.perPage)\n    this.setState({\n      pageCount: Math.ceil(data.length / this.state.perPage),\n      tableData: slice\n    })\n\n  }\n\n\n  componentDidMount() {\n\n    var token = cookies.get('tokenJWT')\n\n    //Requisicao para pegar as informmacoes do usuarios para pegar apenas as requisicoes dele\n    axios.get(process.env.REACT_APP_SERVER_TO_AUTHENTICATE, {\n      method: 'GET',\n      headers: { 'X-access-token': token }\n    }).then((res) => {\n\n\n      if (res.data[0].auth) {\n        //Pegando as informacoes do user pelo nif\n        let url = `${process.env.REACT_APP_SERVER_BASE}/buscar-user-nif/`+ `${res.data[0].nif}`\n\n        axios.get(url).then(async (res) => {\n\n\n          \n\n          this.setState({\n            User_info: res.data\n          })\n\n          //Aqui vai o fetch para a api pegar os registros no banco de dados\n\n\n          let url = `${process.env.REACT_APP_SERVER_BASE}/pegar-requisicao/${this.state.User_info.nif}`\n\n          axios.get(url).then((result) => {\n\n            console.log(result)\n            this.setState({\n              Requisicoes: result.data\n            })\n\n            console.log(\"ola aquii\")\n            console.log(result.data)\n            // slice = de quanto a quanto sera exibido na tela\n            // slice = data.slice(15, 15 + 5) \n            //slice = 20, ou seja na pagina ira commecar a listar pelo numero 20\n            var slice = result.data.slice(this.state.offset, this.state.offset + this.state.perPage)\n            console.log(slice)\n  \n            this.setState({\n              pageCount: Math.ceil(result.data.length / this.state.perPage),\n              orgtableData: result.data,\n              tableData: slice\n            })\n\n\n          })\n\n\n\n\n        }).catch((err) => {\n          console.log(err)\n        })\n      }\n    })\n\n\n\n  }\n\n\n  updateSearch(event) {\n    this.setState({ search: event.target.value.substr(0, 20) })\n  }\n\n\n  render() {\n\n    let filterRegister = this.state.tableData.filter(\n      (register) => {\n        return register.nome.toLowerCase().indexOf(this.state.search) !== -1 ||\n          register.nome.indexOf(this.state.search) !== -1 ||\n          register.nome_requisicao.toLowerCase().indexOf(this.state.search) !== -1 ||\n          register.nome_requisicao.indexOf(this.state.search) !== -1 ||\n          register.data_envio.indexOf(this.state.search) !== -1 ||\n          register.data_entrega.indexOf(this.state.search) !== -1 ||\n          register.nome_departamento.toLowerCase().indexOf(this.state.search) !== -1 ||\n          register.nome_departamento.indexOf(this.state.search) !== -1 ||\n          register.centro_custo.toLowerCase().indexOf(this.state.search) !== -1 \n      }\n    )\n\n    return (\n      <Container>\n\n        <Info>\n          <Title>Historico de Registros</Title>\n          <Pesquisa>\n            <p>pesquisa:</p>\n            <Input\n              value={this.state.search}\n              onChange={this.updateSearch.bind(this)}\n              placeholder=\"Ex: Danilo\" />\n          </Pesquisa>\n        </Info>\n        <Tabela>\n          <table className=\"table table-striped\">\n            <thead>\n              <tr>\n                <th scope=\"col\"><strong>numero</strong></th>\n                <th scope=\"col\"><strong>nome da requisicao</strong></th>\n                <th scope=\"col\"><strong>solicitante</strong></th>\n                <th scope=\"col\"><strong>departamento</strong></th>\n                <th scope=\"col\"><strong>cc</strong></th>\n                <th scope=\"col\"><strong>status</strong></th>\n                <th scope=\"col\"><strong>total de paginas</strong> </th>\n                <th scope=\"col\"><strong>data do pedido</strong></th>\n                <th scope=\"col\"><strong>data de entrega</strong></th>\n                <th scope=\"col\"></th>\n              </tr>\n            </thead>\n            <tbody>\n\n              {\n                filterRegister.map((element) => (\n                  <tr>\n                    <td>{element.id_requisicao}</td>\n                    <td>{element.nome_requisicao}</td>\n                    <td>{element.nome}</td>\n                    <td>{element.nome_departamento}</td>\n                    <td>{element.centro_custo}</td>\n                    <td style={element.status == 'cancelado'? {color:'red'}:{color: 'green'}}>{element.status}</td>\n                    <td>{element.total_paginas}</td>\n                    <td>{element.data_envio}</td>\n                    <td>{element.data_entrega}</td>\n                    <td><SaibaMais caminho=\"detalhes-historicos\" data={element.id_requisicao} /></td>\n\n                  </tr>\n\n                ))\n              }\n\n\n            </tbody>\n          </table>\n        </Tabela>\n        <ReactPaginate\n          previousLabel={\"prev\"}\n          nextLa