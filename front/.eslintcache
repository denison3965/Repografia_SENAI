=> { history.push(\"/\") })\r\n  }, [])\r\n  //**Verificando Se o usuario esta autorizado para acessar essa pagina**\r\n  return (\r\n\r\n    <Container>\r\n\r\n      <Menu_Area>\r\n        <Nav_Lateral ativado=\"1\" />\r\n      </Menu_Area>\r\n\r\n\r\n      {\r\n        showPage\r\n          ?\r\n          <Adm_Area>\r\n            <div className=\"User_Box_Info_Area\">\r\n            <User_Box_Info nome={infoUser.nome} sobrenome={infoUser.sobrenome}/>\r\n              <hr></hr>\r\n\r\n              <Navegation>\r\n                <ul>\r\n                  <Link >\r\n                    <li>Perfil adiministrador</li>\r\n                  </Link>\r\n\r\n                </ul>\r\n              </Navegation>\r\n\r\n            </div>\r\n            <Tabela>\r\n              \r\n              <TabelaHistorico nif={infoUser.nif} />\r\n\r\n              <div className=\"password_box\">\r\n\r\n                <Link to=\"/perfil-adm\"><button type=\"button\" class=\"btn btn-danger\">Voltar</button></Link>\r\n              </div>\r\n            </Tabela>\r\n          </Adm_Area>\r\n\r\n          : <div>\r\n            <div style={loading}>\r\n              <img src={Loading} alt=\"loading\"></img>\r\n            </div>\r\n          </div>\r\n      }\r\n\r\n\r\n    </Container>\r\n\r\n\r\n\r\n\r\n\r\n\r\n  );\r\n}\r\n\r\n\r\n\r\n\r\nexport default Adm_Registros;\r\n\r\n","C:\\Users\\elian\\Desktop\\Repografia_SENAI\\front\\src\\pages\\login\\styles.js",[],"C:\\Users\\elian\\Desktop\\Repografia_SENAI\\front\\src\\pages\\Adm_Registros\\styles.js",[],"C:\\Users\\elian\\Desktop\\Repografia_SENAI\\front\\src\\pages\\Perfil_Historico\\styles.js",[],"C:\\Users\\elian\\Desktop\\Repografia_SENAI\\front\\src\\pages\\Perfil\\styles.js",[],"C:\\Users\\elian\\Desktop\\Repografia_SENAI\\front\\src\\pages\\Detalhes\\styles.js",[],"C:\\Users\\elian\\Desktop\\Repografia_SENAI\\front\\src\\pages\\Detalhes_Historico\\styles.js",[],"C:\\Users\\elian\\Desktop\\Repografia_SENAI\\front\\src\\pages\