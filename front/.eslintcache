orm>\n                                    <div>\n                                       {msg_error != null ? <div className=\"alert alert-danger\">{msg_error} </div> : null}\n                                       {msg_acerto != null ? <div className=\"alert alert-success\">{msg_acerto} </div> : null}\n                                    </div>\n\n                                 </div>\n                              </div>\n                              <div class=\"modal-footer\">\n                                 <button onClick={editarSenha} type=\"button\" class=\"btn btn-primary\">Alterar senha</button>\n                              </div>\n                           </div>\n                        </div>\n                     </div>\n\n\n                     <div className=\"Nova--requisicao\">\n                        <div className=\"posicao_requisicao\">Nova Requisição\n                            <Link to=\"/formulario\"><img src={MaisInfo} className=\"iconeMais\" alt=\"maisInfo\" /></Link>\n                        </div>\n                     </div>\n\n                  </form>\n               </Container>\n               : <div>\n                  <div style={loading}>\n                     <img src={Loading} alt=\"loading\"></img>\n                  </div>\n               </div>\n         }\n      </div>\n\n\n   )\n}\n\nexport default Perfil;","/home/ana/Área de Trabalho/reprografia/Repografia_SENAI/front/src/pages/Detalhes/index.jsx",["291","292","293","294","295","296","297"],"import React from 'react';\nimport Nav_Lateral from '../../components/Nav_Lateral'\nimport User_Box_Info from '../../components/User_Box_Info'\nimport Tabela_Registros from '../../components/Tabela_Registros'\n\nimport { Container, Adm_Area, Menu_Area, Information } from './styles';\nimport { useState } from 'react';\nimport { useEffect } from 'react';\nimport { Info } from '../../components/Tabela_Registros/styles';\n\nimport Impressao from '../../assets/img/impressao.png'\ni